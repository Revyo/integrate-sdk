{
  "name": "integrate-sdk",
  "version": "0.6.5",
  "description": "Type-safe TypeScript SDK for MCP Client with plugin-based OAuth provider configuration",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Revyo/integrate-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Revyo/integrate-sdk/issues"
  },
  "homepage": "https://github.com/Revyo/integrate-sdk#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    },
    "./oauth": {
      "types": "./dist/oauth.d.ts",
      "import": "./dist/oauth.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js"
    },
    "./adapters/astro": {
      "types": "./dist/adapters/astro.d.ts",
      "import": "./dist/adapters/astro.js"
    },
    "./adapters/elysia": {
      "types": "./dist/adapters/elysia.d.ts",
      "import": "./dist/adapters/elysia.js"
    },
    "./adapters/expo": {
      "types": "./dist/adapters/expo.d.ts",
      "import": "./dist/adapters/expo.js"
    },
    "./adapters/express": {
      "types": "./dist/adapters/express.d.ts",
      "import": "./dist/adapters/express.js"
    },
    "./adapters/fastify": {
      "types": "./dist/adapters/fastify.d.ts",
      "import": "./dist/adapters/fastify.js"
    },
    "./adapters/hono": {
      "types": "./dist/adapters/hono.d.ts",
      "import": "./dist/adapters/hono.js"
    },
    "./adapters/lynx": {
      "types": "./dist/adapters/lynx.d.ts",
      "import": "./dist/adapters/lynx.js"
    },
    "./adapters/nestjs": {
      "types": "./dist/adapters/nestjs.d.ts",
      "import": "./dist/adapters/nestjs.js"
    },
    "./adapters/nitro": {
      "types": "./dist/adapters/nitro.d.ts",
      "import": "./dist/adapters/nitro.js"
    },
    "./adapters/node": {
      "types": "./dist/adapters/node.d.ts",
      "import": "./dist/adapters/node.js"
    },
    "./adapters/nuxt": {
      "types": "./dist/adapters/nuxt.d.ts",
      "import": "./dist/adapters/nuxt.js"
    },
    "./adapters/remix": {
      "types": "./dist/adapters/remix.d.ts",
      "import": "./dist/adapters/remix.js"
    },
    "./adapters/solid-start": {
      "types": "./dist/adapters/solid-start.d.ts",
      "import": "./dist/adapters/solid-start.js"
    },
    "./adapters/svelte-kit": {
      "types": "./dist/adapters/svelte-kit.d.ts",
      "import": "./dist/adapters/svelte-kit.js"
    },
    "./adapters/tanstack-start": {
      "types": "./dist/adapters/tanstack-start.d.ts",
      "import": "./dist/adapters/tanstack-start.js"
    },
    "./adapters/waku": {
      "types": "./dist/adapters/waku.d.ts",
      "import": "./dist/adapters/waku.js"
    }
  },
  "files": [
    "dist",
    "index.ts",
    "server.ts",
    "oauth.ts",
    "react.ts"
  ],
  "scripts": {
    "prep": "bun run type-check && bun run build",
    "build": "bun build index.ts server.ts oauth.ts react.ts --outdir dist --target node --format esm --external react && bun run build:adapters && bun run build:types",
    "build:adapters": "bun build src/adapters/*.ts --outdir dist/adapters --target node --format esm",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "dev": "bun --watch src/index.ts",
    "type-check": "tsc --noEmit",
    "test": "bun test tests/",
    "test:watch": "bun test --watch tests/",
    "test:unit": "bun test tests/protocol tests/plugins tests/client tests/integrations tests/oauth tests/transport tests/utils tests/errors",
    "test:integration": "bun test tests/integration/simple-integration.test.ts",
    "test:coverage": "bun test --coverage tests/"
  },
  "keywords": [
    "mcp",
    "client",
    "sdk",
    "typescript",
    "oauth",
    "plugin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.0",
    "react": "^19.2.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "react": ">=19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "simple-git-hooks": {
    "pre-commit": "./scripts/check-version.sh"
  }
}